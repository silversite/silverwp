{
  "name": "silversite/silverwp",
  "type": "wordpress-plugin",
  "description": "Platform to help WordPress developers create plugins and themes",
  "homepage": "https://github.com/silversite/silverwp",
  "license": "GPLv2 or later",
  "authors": [
    {
      "name": "Michal Kalkowski",
      "homepage": "http://silversite.pl"
    }
  ],
  "support": {
    "issues": "https://github.com/silversite/silverwp/issues"
  },
  "keywords": [
    "roots",
    "sage",
    "wordpress",
    "stack",
    "composer",
    "silverwp",
    "wp"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.zendframework.com/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/scribu/wp-posts-to-posts"
    }
  ],
  "require": {
    "php": ">=5.3",
    "abraham/twitteroauth": "^0.6.0",
    "zendframework/zend-inputfilter": "^2.5",
    "zendframework/zend-servicemanager": "^2.6",
    "zendframework/zend-i18n": "^2.5",
    "zendframework/zendservice-recaptcha": "2.0"
  },
  "autoload": {
    "files": [
      "vendor/vafour/vafpress-framework/bootstrap.php",
      "vendor/Taxonomy_Single_Term/class.taxonomy-single-term.php",
      "vendor/scribu/posts-to-posts/posts-to-posts.php"
    ],
    "psr-4": {
      "SilverWp\\": "SilverWp/"
    }
  }
}
